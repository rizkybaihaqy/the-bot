{
  "name": "the-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "babel-node ./src/bin/www.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "prod": "node ./dist/bin/www.js",
    "format": "prettier-eslint --write $PWD/\"src/**/*.js\"",
    "migrate": "node-pg-migrate",
    "migrate-refresh-seed": "npm run migrate down 0 && npm run migrate up && babel-node ./src/db/seeder/sales.js && babel-node ./src/db/seeder/visits.js"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.22.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "fluture": "^14.0.0",
    "fluture-express": "^6.0.1",
    "fluture-sanctuary-types": "^7.1.0",
    "morgan": "~1.9.1",
    "node-pg-format": "^1.3.5",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "regenerator-runtime": "^0.13.9",
    "sanctuary": "^3.1.0",
    "sanctuary-def": "^0.22.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@faker-js/faker": "^7.5.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^6.0.1"
  }
}
