{
  "name": "the-bot",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development babel-node ./src/bin/www.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel ./src --out-dir dist --copy-files",
    "prod": "npm run build && node ./dist/bin/www.js",
    "deta:build": "npm run build && mv ./dist/bin/deta-entry.js ./index.js",
    "deta:deploy": "npm run deta:build && deta deploy && deta update -e .env && rm ./index.js",
    "deta:seed": "npx babel-node ./src/db/deta-base/seeder.js",
    "pg:migrate": "node-pg-migrate --envPath development.env up",
    "pg:fresh": "node-pg-migrate --envPath development.env down 0",
    "pg:seed": "cross-env NODE_ENV=development babel-node ./src/db/postgres/seeder.js",
    "pg:migrate-fresh-seed": "npm run pg:fresh && npm run pg:migrate && npm run pg:seed",
    "lint": "eslint $PWD/\"src/**/*.js\"",
    "format": "prettier-eslint --write $PWD/\"src/**/*.js\"",
    "test": "jest"
  },
  "dependencies": {
    "@types/sanctuary": "^3.1.0",
    "axios": "^0.27.2",
    "change-case": "^4.1.2",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.22.4",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "deta": "^1.1.0",
    "dotenv": "^16.0.0",
    "eta": "^1.13.0",
    "express": "~4.16.1",
    "fluture": "^14.0.0",
    "fluture-express": "^6.0.1",
    "fluture-sanctuary-types": "^7.1.0",
    "morgan": "~1.9.1",
    "node-pg-format": "^1.3.5",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "regenerator-runtime": "^0.13.9",
    "sanctuary": "^3.1.0",
    "sanctuary-def": "^0.22.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.19.1",
    "@faker-js/faker": "^7.5.0",
    "@gipphe/eslint-config-haskellish": "^0.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.0.3",
    "babel-jest": "^29.0.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.0.3",
    "jest-extended": "^3.1.0",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^6.0.1",
    "sanctuary-style": "^5.0.0"
  }
}
