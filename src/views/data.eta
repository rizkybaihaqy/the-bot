<% layout('./layouts/default') %>

<h1><%= it.type %></h1>
<figure>
  <table>
    <thead>
      <tr>
        <% Object.keys(it.data[0]).forEach((prop) => { %>
        <th><%= prop %></th>
        <% }) %>
      </tr>
    </thead>
    <tbody>
      <% it.data.forEach((d) => { %>
      <tr>
        <% Object.keys(d).forEach((key) => { %>
        <td><%= d[key] %></td>
        <% }) %>
      </tr>
      <% }) %>
    </tbody>
  </table>
</figure>
